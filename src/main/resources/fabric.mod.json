{
  "schemaVersion": 1,
  "id": "packchooser",
  "version": "${version}",
  "name": "Pack Chooser",
  "description": "Minecraft Mod that Makes it Easier to Install and Use Datapacks",
  "authors": ["LegibleSleet827", "Cbble_"],
  "contact": {
    "issues": "https://github.com/1ndiigo/pack-chooser/issues",
    "sources": "https://github.com/1ndiigo/pack-chooser"
  },
  "license": "MIT",
  "icon": "assets/packchooser/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "me.cobble.packchooser.client.DatapackChooserClient"
    ],
    "main": [
      "me.cobble.packchooser.DatapackChooser"
    ]
  },
  "mixins": [
    "DatapackChooser.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.8",
    "fabric": "*",
    "minecraft": "1.19"
  }
}
